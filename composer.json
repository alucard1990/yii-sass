{
    "name": "alucard1990/yii-sass",
    "type": "library",
    "description": "Sass (SCSS) and Compass support for the Yii framework",
    "keywords": ["yii","sass","compass","scss","css"],
    "homepage": "https://github.com/artem-frolov/yii-sass",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Frolov",
            "email": "artem@frolov.net",
            "homepage": "http://frolov.net/",
            "role": "Developer"
        }
    ],
    "repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/yourilima/scssphp-compass.git"
    }
  ],
    "require": {
        "php": ">=5.3.0",
        "leafo/scssphp": "~0.3.3",
        "artem-frolov/scssphp-compass": "0.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "phpmd/phpmd" : "*",
        "squizlabs/php_codesniffer": "*",
        "yiisoft/yii": "~1.1"
